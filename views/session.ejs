      <div id="submissionWrapper">
        <div id="bb">
          <form id="idea-submission">
            <input autocomplete="off" type="text" id="idea_field" placeholder="Enter your idea here">
          </form>
        </div>
        <div id="buttonContainer"><a href="#" class="button">Continue</a></div>
      </div>
    </div>
    <script src="/socket.io/socket.io.js"></script>
