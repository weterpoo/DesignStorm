    <meta charset="utf-8">
    <link rel="stylesheet" href="/css/session.css">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>

    <div id="myProgress">
      <div id="myBar"></div>
    </div>
    <div id="navbar">
      <img id="navlogo" src="/img/logo-small.png">
